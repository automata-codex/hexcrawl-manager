{
  "name": "@achm/cli",
  "version": "2.8.0",
  "private": true,
  "scripts": {
    "check": "tsc --noEmit",
    "start": "tsx src/hexcrawl.ts",
    "test": "vitest run --dir src --mode unit",
    "test:int": "vitest run --dir src --mode int",
    "test:scribe": "vitest run --dir src/commands/scribe --mode unit",
    "test:scribe:int": "vitest run --dir src/commands/scribe --mode int",
    "test:scribe:watch": "vitest --watch --dir src/commands/scribe --mode unit",
    "test:watch": "vitest --watch --dir src --mode unit",
    "test:weave": "vitest run --dir src/commands/weave --mode unit",
    "test:weave:int": "vitest run --dir src/commands/weave --mode int",
    "test:weave:watch": "vitest --watch --dir src/commands/weave --mode unit"
  },
  "type": "module",
  "volta": {
    "node": "22.20.0",
    "npm": "11.6.1"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@achm/cli-kit": "*",
    "@achm/core": "*",
    "@achm/data": "*",
    "@achm/schemas": "*",
    "@achm/test-helpers": "*",
    "commander": "^13.1.0",
    "lodash-es": "^4.17.21"
  }
}
