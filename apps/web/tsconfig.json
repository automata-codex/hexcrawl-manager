{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      }
    ],
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    ".astro/types.d.ts",
    "src",
    "astro.config.mjs",
    "svelte.config.js"
  ],
  "exclude": [
    "dist",
  ]
}
