{
  "name": "@skyreach/schemas",
  "version": "0.0.0",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*.schema.json": "./dist/*.schema.json"
  },
  "files": ["dist"],
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.build.json && node ./scripts/build-json-schemas.mjs",
    "clean": "rimraf dist"
  },
  "type": "module",
  "devDependencies": {
    "rimraf": "^6",
    "typescript": "^5.6.3",
    "zod-to-json-schema": "^3"
  },
  "peerDependencies": {
    "zod": "^3.23.8"
  }
}
