{
  "name": "@achm/schemas",
  "version": "5.0.0",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*.schema.json": "./dist/*.schema.json"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:json-schemas": "tsx ./src/build-json-schemas.ts",
    "check": "tsc --noEmit",
    "test": "vitest run --dir src",
    "test:watch": "vitest --watch --dir src"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "dependencies": {
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "rimraf": "^6.0.1",
    "zod-to-json-schema": "^3.24.5"
  }
}
