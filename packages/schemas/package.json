{
  "name": "@skyreach/schemas",
  "version": "2.3.0",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*.schema.json": "./dist/*.schema.json"
  },
  "files": ["dist"],
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:json-schemas": "tsx ./src/build-json-schemas.ts"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "devDependencies": {
    "rimraf": "^6",
    "typescript": "^5.6.3",
    "zod-to-json-schema": "^3"
  },
  "peerDependencies": {
    "zod": "^3.23.8"
  }
}
