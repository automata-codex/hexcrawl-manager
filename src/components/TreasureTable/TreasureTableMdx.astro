---
import { processTreasure } from '../../utils/treasure';

import TreasureTable from './TreasureTable.svelte';

import type { ExtendedTreasureData } from '../../types';
import type { TreasureData } from '@skyreach/schemas';

interface Props {
  treasure: TreasureData[];
}

const { treasure } = Astro.props;
const extendedTreasure: ExtendedTreasureData[] =
  await processTreasure(treasure);
---

<TreasureTable treasure={extendedTreasure} />
