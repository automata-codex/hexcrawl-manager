---
import { processTreasure } from '../../utils/treasure';

import TreasureTable from './TreasureTable.svelte';

import type { ExtendedTreasureData, TreasureData } from '../../types';

interface Props {
  treasure: TreasureData[];
}

const { treasure } = Astro.props;
const extendedTreasure: ExtendedTreasureData[] = await processTreasure(treasure);
---

<TreasureTable treasure={extendedTreasure} />
